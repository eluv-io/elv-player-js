{
  "name": "@eluvio/elv-player-js",
  "version": "1.0.5",
  "description": "![Eluvio Logo](src/static/images/Logo.png \"Eluvio Logo\")",
  "main": "src/index.js",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-fabric-browser-app-template",
  "scripts": {
    "bump-version": "npm --git-tag-version --no-commit-hooks version patch",
    "serve": "TEST_PAGE=true webpack-dev-server --hot --port 8088 --host=0.0.0.0 --disable-host-check --useLocalIp",
    "build": "rm -rf dist/* && NODE_ENV=production node ./node_modules/webpack-cli/bin/cli.js -p --progress --colors --devtool none",
    "build-analyze": "rm -rf dist/* && NODE_ENV=production ANALYZE_BUNDLE=true node ./node_modules/webpack-cli/bin/cli.js -p --progress --colors --devtool none",
    "add-build": "COMMIT=`git rev-parse HEAD` ; echo $COMMIT > dist/version.txt ; git add dist",
    "lint": "npm run fix-scss ; npm run lint-scss ; npm run fix-js",
    "lint-show": "npm run lint-scss ; npm run lint-js",
    "lint-js": "eslint \"src/**/*.js\"",
    "lint-scss": "./node_modules/sass-lint/bin/sass-lint.js -c .scss-lint.yml -v -q",
    "fix-js": "npm run lint-js -- --fix",
    "fix-scss": "./node_modules/sass-lint-auto-fix/dist/index.js"
  },
  "dependencies": {
    "@eluvio/elv-client-js": "^3.1.47",
    "dashjs": "^3.2.1",
    "hls.js": "^0.14.17",
    "lodash": "^4.17.21",
    "urijs": "^1.19.6",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-regenerator": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.3",
    "autoprefixer": "^9.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-preset-mobx": "^2.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^2.0.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^0.5.1",
    "regenerator-runtime": "^0.12.1",
    "sass-lint": "^1.12.1",
    "sass-lint-auto-fix": "^0.21.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.22.1",
    "svg-inline-loader": "^0.8.0",
    "svg-inline-react": "^3.1.0",
    "uuid": "^7.0.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.11.2"
  },
  "babel": {
    "comments": true,
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "syntax-async-functions",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-object-rest-spread",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-regenerator"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eluv-io/elv-player-js.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/eluv-io/elv-player-js/issues"
  }
}
