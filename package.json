{
  "name": "@eluvio/elv-player-js",
  "version": "1.0.137",
  "description": "![Eluvio Logo](lib/static/images/Logo.png \"Eluvio Logo\")",
  "main": "lib/index.js",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/eluv-io/elv-player-js",
  "scripts": {
    "bump-version": "npm --git-tag-version --no-commit-hooks version patch",
    "serve": "vite --host 0.0.0.0 --port 8089",
    "build": "vite build",
    "build-analyze": "vite-bundle-visualizer",
    "preview": "vite preview",
    "build-icons": "node lib/static/icons/BuildIcons.js",
    "lint": "npm run fix-scss ; npm run lint-scss ; npm run fix-js",
    "lint-show": "npm run lint-scss ; npm run lint-js",
    "lint-js": "eslint \"lib/**/*.js\"",
    "lint-scss": "./node_modules/sass-lint/bin/sass-lint.js -c .scss-lint.yml -v -q",
    "fix-js": "npm run lint-js -- --fix",
    "fix-scss": "./node_modules/sass-lint-auto-fix/dist/index.js"
  },
  "pre-commit": {
    "run": [
      "build"
    ]
  },
  "files": [
    "/dist",
    "/lib",
    "LICENSE",
    "README.md",
    "package.json",
    "package-lock.json"
  ],
  "dependencies": {
    "@eluvio/elv-client-js": "^4.0.76",
    "dashjs": "~4.7.0",
    "hls.js": "~1.4.12",
    "lodash": "^4.17.21",
    "mux-embed": "^4.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.16",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "sass": "^1.69.2",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.21.0",
    "sass-loader": "^13.3.2",
    "vite": "^5.0.11",
    "vite-bundle-visualizer": "^1.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eluv-io/elv-player-js.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/eluv-io/elv-player-js/issues"
  }
}
